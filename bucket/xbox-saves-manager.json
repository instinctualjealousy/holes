{
    "version": "4.5.1",
    "description": "Save manager for original Xbox- requires FTP access",
    "homepage": "http://www.xbox-saves.com",
    "license": "Freeware",
    "notes": [
        "Software creates registry entry at \"HKCU\\SOFTWARE\\Classes\\VirtualStore\\MACHINE\\SOFTWARE\\WOW6432Node\\TeamX4\".",
        "That is, if you're not running as admin, otherwise it's \"HKLM\\SOFTWARE\\WOW6432Node\\TeamX4\".",
        "Of course, minus the \"WOW6432Node\" on 32-bit Windows installs. Long-winded, I know."
    ],
    "url": "http://revoltcars.racing/archive/holes/XBSM451.zip",
    "hash": "6d1e0519e3285536b2c3bf4e25df1312931b6f8a5780b8bd15bf415c13c8e7ee",
    "extract_dir": "XBSM",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\pccache\")) { New-Item -ItemType Directory \"$dir\\pccache\" | Out-Null }",
        "if (!(Test-Path \"$persist_dir\\saves\")) { New-Item -ItemType Directory \"$dir\\saves\" | Out-Null }"
    ],
    "shortcuts": [
        [
            "XSaveMgr.exe",
            "XBox-Saves Manager"
        ]
    ],
    "persist": [
        "pccache",
        "saves"
    ]
}
